<%- include('../partials/header') %>
<table id="list">
  <thead>
    <tr>
      <th>Make</th>
      <th>Model</th>
      <th>Item</th>
      <th>Purchase Year</th>
    </tr>
  </thead>
  <tbody>
    <% valuables.forEach(function(v) { %>
      <tr>
        <td><%= v.make %></td> 
        <td><%= v.model %></td> 
        <td><%= v.valuableType %></td> 
        <td><%= v.datePurchased.getFullYear() %></td>
        <td><a href="/valuables/<%= v._id %>">Details</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>