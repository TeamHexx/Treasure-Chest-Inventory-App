<form id="new-form" action="/valuables" method="POST">
  <label>Make:</label>
  <input type="text" name="make" required>
  <label>Model:</label>
  <input type="text" name="model" required>
  <label>Type:</label>
  <input type="text" name="valuableType" required>
  <label for="datePurchased">Date Purchased</label>
  <input type="date" id="datePurchased"
               name="datePurchased" value="01-13-1985" required/>
